    "angular-ui-router": "^0.2.15",
    "bluebird": "https://registry.npmjs.org/bluebird/-/bluebird-2.10.2.tgz",
    "body-parser": "^1.9.1",
    "bootstrap": "^3.3.5",
    "chance": "^1.0.0",
    "coffee-script": "^1.8.0",
    "compression": "^1.2.0",
    "config": "^1.6.0",
    "cookie-parser": "^1.4.0",
    "express": "^4.10.0",
    "font-awesome": "^4.3.0",
