{
  "name": "angular-express-passport-seed",
  "version": "0.0.0",
  "description": "AngularJS/ExpressJS seed",
  "main": "server/bin/web.js",
  "scripts": {
    "start": "forever -w --watchDirectory server -c \"babel-node --debug=5858\" server/bin/web.js",
    "test": "npm run linter && npm run test:server && npm run test:client",
    "client": "webpack --progress --colors --watch",
    "seed": "babel-node scripts/seed",
    "linter": "eslint .",
    "test:server": "cross-env NODE_ENV=test mocha --compilers js:babel-register tests/server/**/*.spec.js",
    "test:client": "cross-env NODE_ENV=test karma start",
    "clean:build": "rimraf dist",
    "clean:bundle": "rimraf bundle",
    "clean": "npm run clean:dist && npm run clean:bundle",
    "copy:server": "copyfiles package.json npm-shrinkwrap.json Procfile dist/server",
    "build:server": "npm run clean:build && npm run copy:server && babel server -d dist/server",
    "build:client": "",
    "build": "npm run build:server && build:client"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/andresmatasuarez/angular-express-passport-seed.git"
  },
  "keywords": [
    "angularjs",
    "expressjs",
    "seed"
  ],
  "author": "Andrés Mata Suárez <amatasuarez@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "4.2.3"
  },
  "bugs": {
    "url": "https://github.com/andresmatasuarez/angular-express-passport-seed/issues"
  },
  "homepage": "https://github.com/andresmatasuarez/angular-express-passport-seed",
